<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="bitsmartminiPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>bt.bitsmartmini.entity.Manufacturer</class>
    <class>bt.bitsmartmini.entity.Licensing</class>
    <class>bt.bitsmartmini.entity.Stockout</class>
    <class>bt.bitsmartmini.entity.ItemsPrice</class>
    <class>bt.bitsmartmini.entity.Sales</class>
    <class>bt.bitsmartmini.entity.RtdItem</class>
    <class>bt.bitsmartmini.entity.Forms</class>
    <class>bt.bitsmartmini.entity.Shelf</class>
    <class>bt.bitsmartmini.entity.Receipt</class>
    <class>bt.bitsmartmini.entity.RefundPolicy</class>
    <class>bt.bitsmartmini.entity.Userlogs</class>
    <class>bt.bitsmartmini.entity.Stockin</class>
    <class>bt.bitsmartmini.entity.SalesDetails</class>
    <class>bt.bitsmartmini.entity.Uom</class>
    <class>bt.bitsmartmini.entity.Business</class>
    <class>bt.bitsmartmini.entity.Customers</class>
    <class>bt.bitsmartmini.entity.Users</class>
    <class>bt.bitsmartmini.entity.Items</class>
    <class>bt.bitsmartmini.entity.Generic</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/bitsmartmini?zeroDateTimeBehavior=CONVERT_TO_NULL"/>
      <!--property name="javax.persistence.jdbc.user" value="root"/-->
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <!--property name="javax.persistence.jdbc.password" value="1234"/-->
    </properties>
  </persistence-unit>
</persistence>
